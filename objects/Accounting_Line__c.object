    <validationRules>
        <fullName>Validate_FTTA_Calender_Year</fullName>
        <active>false</active>
        <description>FTTA Application - Royalties.</description>
        <errorConditionFormula>AND(
RecordType.DeveloperName ='',
NOT(ISBLANK( Calender_Year__c)),
NOT(REGEX(Calender_Year__c,"[0-9]{4}"))
)</errorConditionFormula>
        <errorDisplayField>Calender_Year__c</errorDisplayField>
        <errorMessage>Invalid Year : Enter in the format YYYY.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Funding_Fiscal_Year</fullName>
        <active>true</active>
        <description>PCOR Application. Created to validate the fiscal years.</description>
        <errorConditionFormula>AND(
RecordType.DeveloperName =='PCORS_Funding',
NOT(ISBLANK( Fiscal_Year__c )),
NOT(REGEX(Fiscal_Year__c ,"[0-9- ]{2,9}"))
)</errorConditionFormula>
        <errorDisplayField>Fiscal_Year__c</errorDisplayField>
        <errorMessage>Invalid Budget Fiscal Year(s).</errorMessage>
    </validationRules>
