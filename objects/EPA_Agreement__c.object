    <validationRules>
        <fullName>FTTA_Check_Dates_When_Active</fullName>
        <active>true</active>
        <description>FTTA. Created to check the values for the dates when status is made active.</description>
        <errorConditionFormula>IF(
	AND(
		OR(RecordType.DeveloperName = 'FTTA_EPA_Agreement',RecordType.DeveloperName = 'FTTA_License') , 
		ISPICKVAL(Status__c,'Active'), 
		ISCHANGED(Status__c), 
		Or(
		ISBLANK(Date_Signed__c),
		ISBLANK(Hardcopy_Recv_d_Date__c)
		)
	
	),
	TRUE,FALSE
)</errorConditionFormula>
        <errorMessage>Please select the 'Date Signed','Hardcopy Recv'd Date' for the agreement/license.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FTTA_Check_OGC_Fields_When_Pending</fullName>
        <active>true</active>
        <description>FTTA. Created to validate if the OGC fields are not empty when the status is changed to pending.</description>
        <errorConditionFormula>IF(
AND(
RecordType.DeveloperName = 'FTTA_EPA_Agreement' ,
ISPICKVAL(Status__c,'Pending'),
ISCHANGED(Status__c),
Or(
ISBLANK(Lab_Contact__c),
ISBLANK(Principal_Investigator_1__c),
ISBLANK(Program_Manager_1__c)
)

),
TRUE,FALSE
)</errorConditionFormula>
        <errorMessage>Please select the 'OGC Attorney', 'OGC Review Needed', 'Lab contact', PM and PI for the agreement.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FTTA_Check_Owner_When_Assigned</fullName>
        <active>true</active>
        <description>FTTA. Created to validate if the owner field is not empty when the status is changed to assigned.</description>
        <errorConditionFormula>IF(
	AND(
		OR(RecordType.DeveloperName = 'FTTA_EPA_Agreement',RecordType.DeveloperName = 'FTTA_License') , 
		ISPICKVAL(Status__c,'Assigned'), 
		ISCHANGED( Status__c ), 
		ISBLANK(Owner_Contact__c)
	),
	TRUE,FALSE
)</errorConditionFormula>
        <errorMessage>Please select the 'Owner' for the agreement/license.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FTTA_Validate_Amendment</fullName>
        <active>true</active>
        <description>FTTA. Created to check for the agreement type when amendment is created.</description>
        <errorConditionFormula>OR(
	AND(
	RecordType.DeveloperName ='FTTA_EPA_Agreement',
	NOT(ISBLANK( Agreement__c )),
	TEXT( Agreement_Type__c )&lt;&gt;'Amendment'
	),
	AND(
	RecordType.DeveloperName ='FTTA_License',
	NOT(ISBLANK( Agreement__c )),
	TEXT( License_Type__c  )&lt;&gt;'Amendment'
	)
)</errorConditionFormula>
        <errorMessage>Invalid Agreement/License type. Only Amendment can be selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FTTA_Validate_Amendment_to_Amendment</fullName>
        <active>true</active>
        <description>FTTA. Created to check if amendment is created to an amendment.</description>
        <errorConditionFormula>OR(
	AND(
	RecordType.DeveloperName ='FTTA_EPA_Agreement',
        NOT(ISBLANK( Agreement__r.Agreement__c )),
	TEXT( Agreement_Type__c )='Amendment'
	),
	AND(
	RecordType.DeveloperName ='FTTA_License',
        NOT(ISBLANK( Agreement__r.Agreement__c )),
	TEXT( License_Type__c  )='Amendment'
	)
)</errorConditionFormula>
        <errorMessage>Amendments cannot be created for an amendment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>License_Check_OGC_Fields_When_Pending</fullName>
        <active>true</active>
        <description>FTTA License. Created to validate if the OGC fields are not empty when the status is changed to pending.</description>
        <errorConditionFormula>IF(
AND(
RecordType.DeveloperName = 'FTTA_License' ,
ISPICKVAL(Status__c,'OGC review'),
ISCHANGED(Status__c),
ISBLANK(OGC_Attorney__c )

),
TRUE,FALSE
)</errorConditionFormula>
        <errorMessage>Please select the 'OGC Attorney' for the license.</errorMessage>
    </validationRules>
