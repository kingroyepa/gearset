    <validationRules>
        <fullName>Citation_Length</fullName>
        <active>true</active>
        <errorConditionFormula>LEN(Citation__c) &gt; 120</errorConditionFormula>
        <errorDisplayField>Citation__c</errorDisplayField>
        <errorMessage>Citation Required. Length must not exceed 120 characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Limit_2_Category_Selection</fullName>
        <active>true</active>
        <errorConditionFormula>IF(INCLUDES( Secondary_Research_Category__c , "	Ecological Research"),1,0) + 
IF(INCLUDES( Secondary_Research_Category__c, "Environmental Futures"),1,0) + 
IF(INCLUDES( Secondary_Research_Category__c, "Environmental Statistics"),1,0) + 
IF(INCLUDES( Secondary_Research_Category__c, "HEResearch and HHRisk Assessment"),1,0) + 
IF(INCLUDES( Secondary_Research_Category__c, "Integrated Risk Assessment"),1,0) + 
IF(INCLUDES( Secondary_Research_Category__c, "Monitoring and Measuring Methods"),1,0) + 
IF(INCLUDES( Secondary_Research_Category__c, "Other Environmental Research"),1,0) + 
IF(INCLUDES( Secondary_Research_Category__c, "Review Articles"),1,0) + 
IF(INCLUDES( Secondary_Research_Category__c, "Social Sciences"),1,0) +
IF(INCLUDES( Secondary_Research_Category__c, "Transport and Fate"),1,0) &gt; 2</errorConditionFormula>
        <errorDisplayField>Secondary_Research_Category__c</errorDisplayField>
        <errorMessage>Limit of two categories</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>STAA_Locked_Record_based_on_Status</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(

OR(
TEXT(Status__c) = 'FINAL',
TEXT(Status__c) = 'AWARDED',
TEXT(Status__c) = 'NOT AWARDED',
TEXT(Status__c) = 'EXPIRED'),

OR(
 ISCHANGED(Primary_Research_Category__c),
 ISCHANGED(Secondary_Research_Category__c), 
 ISCHANGED(Nominating_Official__c),
 ISCHANGED(STAA_Coordinator__c), 
 ISCHANGED(OwnerId),
 ISCHANGED(Laboratory_Office_Name__c),
 ISCHANGED(Citation__c),
 ISCHANGED(Eligibility_Criteria_1__c),
 ISCHANGED(Eligibility_Criteria_2__c),
 ISCHANGED(Eligibility_Criteria_3__c),
 ISCHANGED(Eligibility_Criteria_4__c),
 ISCHANGED(Justification_1__c),
 ISCHANGED(Justification_2A__c),
 ISCHANGED(Justification_2B__c),
 ISCHANGED(Justification_2C__c),
 ISCHANGED(Justification_2D__c),
 ISCHANGED(Justification_2E__c),
 ISCHANGED(Justification_3A__c),
 ISCHANGED(Justification_3B__c)
)),

TRUE, 
FALSE)</errorConditionFormula>
        <errorMessage>This record is currently LOCKED from editing</errorMessage>
    </validationRules>
