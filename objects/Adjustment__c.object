    <validationRules>
        <fullName>DCR_Op_Plan_Adjustments_Final</fullName>
        <active>true</active>
        <description>Does not allow the user to edit the record after its status changes to 'Final'</description>
        <errorConditionFormula>AND (
OR (
		ISCHANGED(Amount__c),
	 ISCHANGED(Op_Plan_Negative__c),
  ISCHANGED(Op_Plan_Positive__c),
		TEXT(PRIORVALUE(Budget_Adjustment_Type__c)) &lt;&gt; TEXT(Budget_Adjustment_Type__c),
		TEXT(PRIORVALUE(Status__c)) &lt;&gt; TEXT(Status__c)),
		TEXT(PRIORVALUE(Status__c)) = "Final",
RecordType.DeveloperName = "DCR_Op_Plan_Adjustment"
		)</errorConditionFormula>
        <errorMessage>The Adjustment record cannot be changed after it has been marked as final (i.e. processed).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DCR_Op_Plans_Cannot_be_Same</fullName>
        <active>true</active>
        <description>The Op Plans in a DCR adjustment cannot be the same record.</description>
        <errorConditionFormula>OR(  RecordType.Name = "DCR Op Plan Adjustment", RecordType.Name = "DCR Suballocation Adjustment" ) &amp;&amp;  Op_Plan_Negative__c = Op_Plan_Positive__c</errorConditionFormula>
        <errorMessage>The Op Plan (+) and Op Plan (-) fields cannot reference the same document. Please adjust one of the fields or cancel the operation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Op_Plan_Adjustment_Values_Cannot_be_Null</fullName>
        <active>true</active>
        <description>Op plan adjustment values cannot be null for Op Plan and Suballocation record types when executed.</description>
        <errorConditionFormula>OR( RecordType.DeveloperName = "DCR_Op_Plan_Adjustment", RecordType.DeveloperName = "DCR_Suballocation_Adjustment" )
&amp;&amp;
AND (ISBLANK( Op_Plan_Negative__c ), ISBLANK( Op_Plan_Positive__c ) )
&amp;&amp;
ISPICKVAL( Status__c, "Final" )</errorConditionFormula>
        <errorMessage>The Op Plan (-) and (+) fields cannot both be null when trying to execute an adjustment. Please enter Op Plan or Suballocation records one of the fields.</errorMessage>
    </validationRules>
