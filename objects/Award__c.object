    <validationRules>
        <fullName>ARTT_Cannot_Bulk_Upload_5k</fullName>
        <active>false</active>
        <description>Cannont upload an award 5k or more, or an award that sends the awardee over 5k for the year.</description>
        <errorConditionFormula>(RecordType.DeveloperName = "Cash_Award") &amp;&amp;
    (Total_Award_Amount_on_Submit__c &gt; 5000) 
        &amp;&amp;  Include_in_Bulk_Upload__c = TRUE</errorConditionFormula>
        <errorMessage>An award $5k or more, or an award that sends the awardee over $5k cannot be bulk uploaded.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ARTT_FPPS_Change</fullName>
        <active>false</active>
        <description>Restricts the users from changing the FPPS # out of process.</description>
        <errorConditionFormula>NOT( OR(ISPICKVAL(Status__c, "Ready for FPPS"), ISPICKVAL(Status__c, "To SSC"), ISPICKVAL(Status__c, "Paid"),  ISPICKVAL(Status__c, "Reference")) ) &amp;&amp; ( ISCHANGED(FPPS_Number__c) )</errorConditionFormula>
        <errorMessage>You cannot update the FPPS # prior to the Ready for FPPS status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ARTT_To_OHR_Change</fullName>
        <active>true</active>
        <description>Restricts the users from changing the Sent to OHR field out of process.</description>
        <errorConditionFormula>NOT( OR(ISPICKVAL(Status__c, "Ready for SSC"), ISPICKVAL(Status__c, "To SSC"), ISPICKVAL(Status__c, "Paid"), ISPICKVAL(Status__c, "Funded"), ISPICKVAL(Status__c, "Reference")) ) &amp;&amp; OR( ISCHANGED( Sent_to_OHR__c ))</errorConditionFormula>
        <errorMessage>You cannot update the FPPS # prior to the Ready for SSC or To SSC statuses.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Change_Group_Award_After_Submit</fullName>
        <active>true</active>
        <description>Prohibits users from changing the group award lookup post submission</description>
        <errorConditionFormula>(NOT ISPICKVAL(Status__c, "Planned" )) &amp;&amp; ISCHANGED(Group_Award_Name__c)</errorConditionFormula>
        <errorMessage>You cannot add the award or remove the award from a Group Award post submission.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contractors_Cannot_Receive_Awards</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISPICKVAL(Emp_Name__r.Person_Type__c, "Contractor"),
ISPICKVAL(Emp_Name__r.Person_Type__c, "External"))</errorConditionFormula>
        <errorMessage>Contractors or External users cannot receive awards.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Entered_into_FPPS_and_FPPS_Num_Blank</fullName>
        <active>true</active>
        <description>You must include the FPPS # if the award is entered into FPPS.</description>
        <errorConditionFormula>ISCHANGED( Entered_into_FPPS__c) &amp;&amp; ISBLANK( FPPS_Number__c)</errorConditionFormula>
        <errorMessage>You must include the FPPS # if you're entering the award into FPPS.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Entered_into_FPPS_and_Include_in_Bulk</fullName>
        <active>true</active>
        <description>You cannot include in bulk upload and enter into FPPS</description>
        <errorConditionFormula>Entered_into_FPPS__c = TRUE &amp;&amp;   Include_in_Bulk_Upload__c = TRUE</errorConditionFormula>
        <errorMessage>You cannot include the award in bulk upload and enter the award into FPPS.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Group_Awards_Can_Not_be_Bulk_Uploaded</fullName>
        <active>true</active>
        <description>Group awards cannot be bulk uploaded, so this stops users from selected add to bulk upload.</description>
        <errorConditionFormula>NOT ISBLANK( Group_Award_Name__c ) &amp;&amp;  Include_in_Bulk_Upload__c = TRUE</errorConditionFormula>
        <errorMessage>Group Awards cannot be bulk uploaded. Please enter the group awards into FPPS.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HR_Review_Order</fullName>
        <active>true</active>
        <description>Cannot edit the HR Reviewed checkbox outside of HR Review</description>
        <errorConditionFormula>NOT(ISPICKVAL(Status__c, "HR Review") ) &amp;&amp; ( ISCHANGED(  HR_Reviewed__c ) )</errorConditionFormula>
        <errorMessage>You cannot edit HR Reviewed outside of the HR Review status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NonAdmins_Cannot_Change_Dates_and_Status</fullName>
        <active>false</active>
        <description>Stops Non-Admins from editing dates and status. Needed because they have perm set level permissions on the fields so they can run screen flows, but can't change in the record.</description>
        <errorConditionFormula>NOT( $Permission.Awards_App_Admin_Access ) &amp;&amp; Bypass_Validation__c = FALSE &amp;&amp;
OR( ISCHANGED(  Date_Entered_into_FPPS__c ), 
ISCHANGED(  Date_Ready_for_Bulk_Upload__c ), 
ISCHANGED( Date_Ready_for_FPPS__c ),
ISCHANGED( IOAA_Approval_Date__c ), 
ISCHANGED( Submitted_Date__c ), 
ISCHANGED( Supervisor_Approval_Date__c ),
ISCHANGED(Union_Review_Date__c), 
ISCHANGED(  Status__c  ) )</errorConditionFormula>
        <errorMessage>You cannot change date or Status fields if you are not an admin.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_Planned_Award_Cannot_Change_Owners</fullName>
        <active>true</active>
        <description>An award that is not in the Planned stage cannot change owners.</description>
        <errorConditionFormula>ISCHANGED( OwnerId ) &amp;&amp; NOT(  ISPICKVAL(Status__c, "Planned") )</errorConditionFormula>
        <errorMessage>An award not in a Planned stage cannot change owners. Please contact administrative support for any questions and/or guidance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_Planned_Award_Cannot_Change_RO</fullName>
        <active>true</active>
        <description>An award that is not in the Planned stage cannot change Requesting Official.</description>
        <errorConditionFormula>ISCHANGED(  Requesting_Official__c  ) &amp;&amp; NOT(  ISPICKVAL(Status__c, "Planned") )</errorConditionFormula>
        <errorMessage>An award not in a Planned stage cannot change Requesting Officials. Please contact administrative support for any questions and/or guidance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OTS_Max_Amount</fullName>
        <active>true</active>
        <description>Does not allow the user to enter an OTS amount greater than 250</description>
        <errorConditionFormula>AND((RecordType.DeveloperName = "OTS_Award"), 
 OTS_Amount__c &gt;250)</errorConditionFormula>
        <errorMessage>You cannot enter an OTS amount greater than $250</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_Award_1_or_2_Can_be_Bulk_Uploaded</fullName>
        <active>true</active>
        <description>Prohibits users from bulk uploading the the third, fourth... award.</description>
        <errorConditionFormula>OR( (RecordType.DeveloperName = 'Cash_Award' &amp;&amp; Include_in_Bulk_Upload__c = TRUE &amp;&amp;  OR(Cash_Award_Counter__c &gt;2, ISBLANK(Cash_Award_Counter__c))),
(RecordType.DeveloperName = 'Time_Off_Award' &amp;&amp; Include_in_Bulk_Upload__c = TRUE) &amp;&amp;  OR(TO_Award_Counter__c &gt;2, ISBLANK(TO_Award_Counter__c)))</errorConditionFormula>
        <errorMessage>Only the first or second award for an employee can be included in the bulk upload. Please enter subsequent awards into FPPS.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pay_Plan_Bulk_Upload_Restrictions</fullName>
        <active>false</active>
        <description>SL, ST, and AD Pay Plans cannot be included in the bulk upload</description>
        <errorConditionFormula>OR( Pay_Plan__c ="AD", Pay_Plan__c ="SL", Pay_Plan__c ="ST") &amp;&amp; Include_in_Bulk_Upload__c = TRUE</errorConditionFormula>
        <errorMessage>SL, ST, or Title 42 S-Awards cannot be included in the bulk upload.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Amount_Change_Post_Submit</fullName>
        <active>true</active>
        <description>Prevent a user from changing any of the award amount fields if the award is not in the Planned stage.</description>
        <errorConditionFormula>NOT( $Permission.Awards_App_Admin_Access ) &amp;&amp; NOT( OR( ISPICKVAL(Status__c, "Planned" ), ISPICKVAL( Status__c, "Rejected" )) ) &amp;&amp; OR( ISCHANGED( Cash_Award_Amount__c ),  ISCHANGED( Time_Off_Amount__c ), ISCHANGED( Group_Award_Amount__c ), ISCHANGED(  Emp_Name__c  ), ISCHANGED(  Requesting_Official__c ),
ISCHANGED(  Authorizing_Official__c  ), ISCHANGED( Budget__c )  )</errorConditionFormula>
        <errorMessage>You cannot change the award amount value, Awardee, Requesting Official, Authorizing Official, or Budget after the award has been submitted or approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ready_for_FPPS_Order</fullName>
        <active>true</active>
        <description>Cannot change entered into fpps, include in bulk upload out of order. FPPS # can be changed at any time in case of correction.</description>
        <errorConditionFormula>NOT(ISPICKVAL(Status__c, "Ready for FPPS")) &amp;&amp; OR(ISCHANGED( Entered_into_FPPS__c), ISCHANGED(Include_in_Bulk_Upload__c))</errorConditionFormula>
        <errorMessage>You can only change Entered into FPPS or Including in Bulk Upload during Ready for FPPS status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Submitting_Awards_Without_Budget</fullName>
        <active>true</active>
        <description>Awards cannot be submitted if they are not associated with a budget.</description>
        <errorConditionFormula>ISPICKVAL(Status__c, "Submitted") &amp;&amp; ISBLANK(Budget__c)</errorConditionFormula>
        <errorMessage>An award cannot be submitted if it is not associated with a budget. Please associate the award with a budget and try to submit again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Submitting_Awards_Without_Justification</fullName>
        <active>false</active>
        <description>Prevent submitting an award without proper justification.</description>
        <errorConditionFormula>AND(OR(ISPICKVAL(Status__c, "Submitted"),ISPICKVAL(Status__c, "Received By OARS/Approved")),  NOT(ISPICKVAL(PRIORVALUE(Status__c), "Submitted")),
OR(ISBLANK(TEXT(Type_of_Benefit__c)), ISBLANK(TEXT(Value_of_Benefit__c)), ISBLANK(TEXT(Extent_of_Contribution__c)), ISNULL(Supervisor_Justification__c)))</errorConditionFormula>
        <errorMessage>Award must have all of Type and Value of Benefit, Extent of Contribution, and Supervisor Justification filled out before submitting. Please check those fields for completion and try submitting again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Submitting_Cash_Awards</fullName>
        <active>true</active>
        <description>Prevent submitting an cash award without proper justification.</description>
        <errorConditionFormula>AND( RecordType.DeveloperName = "Cash_Award", AND(OR(ISPICKVAL(Status__c, "Submitted"),ISPICKVAL(Status__c, "HR Review")),  NOT(ISPICKVAL(PRIORVALUE(Status__c), "Submitted")),
OR(ISBLANK(TEXT(Type_of_Benefit__c)), ISBLANK(TEXT( Value_and_Application__c ))), ISNULL(Supervisor_Justification__c)))</errorConditionFormula>
        <errorMessage>Award must have all of Type, Value and Application,  and Supervisor Justification filled out before submitting. Please check those fields for completion and try submitting again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Submitting_TO_Awards</fullName>
        <active>true</active>
        <description>Prevent submitting an TO award without proper justification.</description>
        <errorConditionFormula>AND( RecordType.DeveloperName = "Time_Off_Award", AND(OR(ISPICKVAL(Status__c, "Submitted"),ISPICKVAL(Status__c, "HR Review")),  NOT(ISPICKVAL(PRIORVALUE(Status__c), "Submitted")),
ISNULL(Supervisor_Justification__c)))</errorConditionFormula>
        <errorMessage>Time off Awards must have a Supervisor Justification filled out before submitting. Please check those fields for completion and try submitting again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TO_Awards_Bulk_Upload_Rules</fullName>
        <active>false</active>
        <description>TO Awards that are not from the Awardees Org Cannot be bulk uploaded</description>
        <errorConditionFormula>RecordType.DeveloperName = "Time_Off_Award" &amp;&amp;  Include_in_Bulk_Upload__c = TRUE &amp;&amp;  
RO_HR_Org__c &lt;&gt;  Emp_Name__r.Account.Name</errorConditionFormula>
        <errorMessage>Time Off Awards cannot be bulk uploaded if the RO's Org is not the same as the Awardee's.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Time_Off_Greater_than_80_Hours</fullName>
        <active>true</active>
        <description>Validation rule that prevents a user from saving or submitting an award that is greater than 80 hours.</description>
        <errorConditionFormula>Time_Off_Amount__c &gt; 80</errorConditionFormula>
        <errorMessage>A time off award cannot exceed 80 hours. Please update the time off amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Union_Review_Order</fullName>
        <active>false</active>
        <description>Cannot change union review status out of order</description>
        <errorConditionFormula>NOT( OR(ISPICKVAL(Status__c, "HR Review"), ISPICKVAL(Status__c, "Sent for Union Review")) ) &amp;&amp; ( ISCHANGED( Union_Review_Status__c ) )</errorConditionFormula>
        <errorMessage>You can only change Union Review Status during HR Review and Sent for Union Review statuses.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Cash_Award_Amount</fullName>
        <active>false</active>
        <description>The cash award amount must be 1.2x the OTS amount if the OTS amount field is populated.</description>
        <errorConditionFormula>OR((OTS_Amount__c &lt;&gt; 0 &amp;&amp; Cash_Award_Amount__c &lt;&gt; 0 &amp;&amp; Cash_Award_Amount__c &lt; (1.2 * OTS_Amount__c)), OTS_Amount__c &lt;&gt; 0 &amp;&amp; Cash_Award_Amount__c == 0)</errorConditionFormula>
        <errorMessage>The OTS Amount must be at least 1.2x greater than the Cash Award Amount or the Cash Award Amount must be greater than 0 if there is an OTS Amount. Please update the amounts and try saving again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Whole_Dollar_Cash_Amounts</fullName>
        <active>true</active>
        <description>Cash award amounts must be whole dollar amounts</description>
        <errorConditionFormula>AND((RecordType.DeveloperName = "Cash_Award"), 
NOT(Len(Text(Cash_Award_Amount__c - (ROUND(Cash_Award_Amount__c ,0))))=1))</errorConditionFormula>
        <errorMessage>You must enter a whole dollar amount for cash awards.</errorMessage>
    </validationRules>
