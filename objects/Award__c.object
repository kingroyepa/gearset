    <fields>
        <fullName>Award_Fiscal_Year__c</fullName>
        <description>ARTT FY of the award based on submission date or planned date if the award has not been submitted yet.</description>
        <externalId>false</externalId>
        <formula>TEXT(IF(ISBLANK( Submitted_Date__c ),  
CASE( MONTH(Planned_Date__c), 
1, (YEAR(Planned_Date__c)), 
2, (YEAR(Planned_Date__c)), 
3, (YEAR(Planned_Date__c)), 
4, (YEAR(Planned_Date__c)), 
5, (YEAR(Planned_Date__c)), 
6, (YEAR(Planned_Date__c)), 
7, (YEAR(Planned_Date__c)), 
8, (YEAR(Planned_Date__c)), 
9, (YEAR(Planned_Date__c)), 
(YEAR(Planned_Date__c))+1), 
CASE( MONTH(Submitted_Date__c), 
1, (YEAR(Submitted_Date__c)), 
2, (YEAR(Submitted_Date__c)), 
3, (YEAR(Submitted_Date__c)), 
4, (YEAR(Submitted_Date__c)), 
5, (YEAR(Submitted_Date__c)), 
6, (YEAR(Submitted_Date__c)), 
7, (YEAR(Submitted_Date__c)), 
8, (YEAR(Submitted_Date__c)), 
9, (YEAR(Submitted_Date__c)), 
(YEAR(Submitted_Date__c))+1) 
))</formula>
        <label>Award Fiscal Year</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
