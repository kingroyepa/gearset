    <validationRules>
        <fullName>Coordinator_Edit_Approved_Record_Rule</fullName>
        <active>true</active>
        <description>Only Coordinators with super use permission set can edit records that have been final approved.</description>
        <errorConditionFormula>AND(
Final_Approved__c,
NOT(( $Permission.TQB_Edit_Approved_Records ) || ($Profile.Name="System Administrator") || $User.Alias="ETL.BAP"
)
)</errorConditionFormula>
        <errorMessage>A TQB Candidate Package cannot be edited once it has been approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TQB_Check_Promotion_Effective_Date</fullName>
        <active>false</active>
        <description>TQB Application. Checks if the promotion effective date is entered before the approval.</description>
        <errorConditionFormula>AND(
 RecordType.DeveloperName ='Candidate_Package',
 TEXT(Status__c)&lt;&gt; 'Approved_by_TQB_Chair',
 TEXT(Status__c)&lt;&gt; 'Completed',
 NOT(ISBLANK( Promotion_Effective_Date__c )) 
)</errorConditionFormula>
        <errorMessage>Promotion Effective Date cannot be entered  before application is approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TQB_Check_TQB_Scheduled_Date</fullName>
        <active>true</active>
        <description>TQB Application. Checks if the TQB Scheduled Date is entered before the approval.</description>
        <errorConditionFormula>AND(
 RecordType.DeveloperName ='Candidate_Package',
 TEXT(Status__c)&lt;&gt; 'Approved_by_TQB_Chair',
 TEXT(Status__c)&lt;&gt; 'TQB Scheduled',
 TEXT(Status__c)&lt;&gt; 'TQB Held',
 TEXT(Status__c)&lt;&gt; 'Rejected',
 TEXT(Status__c)&lt;&gt; 'Recalled',
 TEXT(Status__c)&lt;&gt; 'Completed',
 NOT(ISBLANK( TQB_Scheduled_Date__c )) 
)</errorConditionFormula>
        <errorDisplayField>TQB_Scheduled_Date__c</errorDisplayField>
        <errorMessage>TQB Scheduled Date is entered after the application is approved by TQB chair,</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TQB_Lock_Record_For_Editing_On_Complete</fullName>
        <active>true</active>
        <description>TQB Application. Locks the record for editing for everyone except system administrator upon status complete.</description>
        <errorConditionFormula>AND(
 $Profile.Name &lt;&gt; 'System Administrator',
 $User.Alias &lt;&gt; 'ETL.BAP',
 RecordType.DeveloperName ='Candidate_Package',
 TEXT(Status__c)= 'Completed',
OR(
 ISCHANGED( Candidate__c ), 
 ISCHANGED( Current_GS_Level__c ), 
 ISCHANGED( Permanent_Organization__c ), 
 ISCHANGED( Intended_Track__c ), 
 ISCHANGED( Status__c ), 
 ISCHANGED( Promotion_Effective_Date__c ), 
 ISCHANGED( Comments__c ), 
 ISCHANGED( First_Line_Supervisor__c ), 
 ISCHANGED( Second_Line_Supervisor__c ), 
 ISCHANGED( TQB_Chair__c ), 
 ISCHANGED( TQB_Chair_Comments__c ), 
 ISCHANGED( ProgramRegion_Representative__c ), 
ISCHANGED(  TQB_Coordinator__c ), 
 ISCHANGED(  TQB_Coordinator_2__c ), 
 ISCHANGED(  TQB_Peer__c ), 
 ISCHANGED(  Panel_Member_1_Ad_Hoc__c ), 
 ISCHANGED(  Panel_Member_2_Ad_Hoc__c ), 
 ISCHANGED(  Promotion_Decision__c ), 
 ISCHANGED(  Panel_Recomendation__c )
)
)</errorConditionFormula>
        <errorMessage>The application is locked from editing.</errorMessage>
    </validationRules>
