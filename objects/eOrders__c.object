    <validationRules>
        <fullName>ORDGAS_Check_Archive</fullName>
        <active>true</active>
        <description>GAS application.</description>
        <errorConditionFormula>OR(
AND(
RecordType.DeveloperName ='GAS_eOrder_Admin',
NOT(TEXT( Status__c)='Order Closed'),
ISCHANGED( Archive__c )),
AND(
RecordType.DeveloperName ='GAS_eOrder_Return',
NOT(TEXT( Status__c)='Return Order Closed'),
ISCHANGED( Archive__c ))

)</errorConditionFormula>
        <errorDisplayField>Archive__c</errorDisplayField>
        <errorMessage>The eOrder request can be archived only after the order request is fulfilled.</errorMessage>
    </validationRules>
