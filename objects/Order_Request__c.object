    <validationRules>
        <fullName>ORDGAS_Limit_Delivery_Days_HSF</fullName>
        <active>true</active>
        <errorConditionFormula>IF( RecordType.DeveloperName = 'GAS_Order_Request' &amp;&amp; TEXT(Delivery_facilities__c) = "HSF",

OR (
WEEKDAY( Delivery_Date__c ) = 1,
WEEKDAY( Delivery_Date__c ) = 2,
WEEKDAY( Delivery_Date__c ) = 3,
WEEKDAY( Delivery_Date__c ) = 5,
WEEKDAY( Delivery_Date__c ) = 6,
WEEKDAY( Delivery_Date__c ) = 7)
,FALSE)</errorConditionFormula>
        <errorDisplayField>Delivery_Date__c</errorDisplayField>
        <errorMessage>Delivery Date for Human Studies Facility is only on Wednesdays</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ORDGAS_Limit_Delivery_Days_RTF</fullName>
        <active>true</active>
        <errorConditionFormula>IF( RecordType.DeveloperName = 'GAS_Order_Request' &amp;&amp; TEXT(Delivery_facilities__c) = "Page Rd",

OR (WEEKDAY( Delivery_Date__c ) = 1,
WEEKDAY( Delivery_Date__c ) = 2,
WEEKDAY( Delivery_Date__c ) = 4,
WEEKDAY( Delivery_Date__c ) = 6,
WEEKDAY( Delivery_Date__c ) = 7)

,FALSE)</errorConditionFormula>
        <errorDisplayField>Delivery_Date__c</errorDisplayField>
        <errorMessage>Delivery Date for Page Road is only on Tuesdays &amp; Thursdays.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ORDGAS_Limit_Delivery_Days_RTP</fullName>
        <active>true</active>
        <errorConditionFormula>IF( RecordType.DeveloperName = 'GAS_Order_Request' &amp;&amp; TEXT(Delivery_facilities__c) = "RTP",

OR (WEEKDAY( Delivery_Date__c ) = 1,
WEEKDAY( Delivery_Date__c ) = 2,
WEEKDAY( Delivery_Date__c ) = 4,
WEEKDAY( Delivery_Date__c ) = 6,
WEEKDAY( Delivery_Date__c ) = 7)

,FALSE)</errorConditionFormula>
        <errorDisplayField>Delivery_Date__c</errorDisplayField>
        <errorMessage>Delivery Date for Research Triangle Park is only on Tuesdays &amp; Thursdays</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ORDGAS_Limit_Delivery_Days_Today</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
AND( RecordType.DeveloperName = 'GAS_Order_Request',
TEXT(Stage__c)='New Order',
Delivery_Date__c &lt; TODAY()),

TRUE,FALSE)</errorConditionFormula>
        <errorDisplayField>Delivery_Date__c</errorDisplayField>
        <errorMessage>Delivery Date must be after Today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PCORS_EPEAT_checked</fullName>
        <active>true</active>
        <description>If EPEAT is yes then request type needs to be IT/E&amp;It purchase</description>
        <errorConditionFormula>AND(
    NOT(ISPICKVAL( Request_Type__c , "IT/E&amp;IT Purchase")),
    ISPICKVAL( EPEAT__c , 'YES')
)</errorConditionFormula>
        <errorMessage>If your purchase is EPEAT, Request Type needs to be changed to IT Purchase.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PCORS_EPEAT_validation</fullName>
        <active>true</active>
        <description>PCORS 1.5</description>
        <errorConditionFormula>AND(
    ISPICKVAL( Request_Type__c , 'IT/E&amp;IT Purchase'),
    Green_Purchases__c = TRUE,
    ISPICKVAL(EPEAT__c, "")
)</errorConditionFormula>
        <errorDisplayField>EPEAT__c</errorDisplayField>
        <errorMessage>Please select an EPEAT value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PCORS_GP_Order</fullName>
        <active>true</active>
        <description>Require GP Other to be filled if EPEAT equals no</description>
        <errorConditionFormula>AND(
ISPICKVAL( Request_Type__c , 'IT/E&amp;IT Purchase'),
Green_Purchases__c = TRUE,
ISPICKVAL(EPEAT__c, "NO"),
ISBLANK( GP_Other__c )
)</errorConditionFormula>
        <errorDisplayField>GP_Other__c</errorDisplayField>
        <errorMessage>GP Other is required if not an EPEAT purchase</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PCOR_ITReviewer_Required_When_Type_IT</fullName>
        <active>true</active>
        <description>PCOR Application. Created to check if It “IMO/IT Reviewer” is selected when “IT/E&amp;IT” purchase type is selected.</description>
