NOT(REGEX(Vendor_Phone__c ,"(\\D?[0-9]{3}\\D?)[\\s][0-9]{3}-[0-9]{4}"))
)</errorConditionFormula>
        <errorDisplayField>Vendor_Phone__c</errorDisplayField>
        <errorMessage>Invalid Phone Number. Enter in the format (XXX) XXX-XXXX.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PCOR_Validate_Dates_Entered</fullName>
        <active>true</active>
        <description>PCOR Application. Created to prevent the users from entering the dates before the request is approved.</description>
        <errorConditionFormula>IF( RecordType.DeveloperName = 'Purchase_Card_Order_Request'  , 

AND(
NOT(ISPICKVAL(PRIORVALUE(Request_Status__c),'Approved')),
OR(
ISCHANGED(  Date_Allocated__c ),
ISCHANGED(  Date_Ordered__c ),
ISCHANGED(  Date_Received__c )
)
)


,FALSE)</errorConditionFormula>
        <errorMessage>Enter the dates after the request is approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PCOR_Validate_InternationalAddress</fullName>
        <active>true</active>
        <description>PCOR Application Created to validate the website.</description>
        <errorConditionFormula>AND(
RecordType.DeveloperName ='Purchase_Card_Order_Request',
International_Addresses__c,
ISBLANK(International_Addresses_Text__c ) 
)</errorConditionFormula>
        <errorDisplayField>International_Addresses_Text__c</errorDisplayField>
        <errorMessage>'International Address' text box cannot be left blank when 'International Address' checkbox is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PCOR_Validate_MandatorySources</fullName>
        <active>true</active>
        <description>PCOR Application. Created  to check if atleast one mandatory source is selected.</description>
        <errorConditionFormula>AND(RecordType.DeveloperName =='Purchase_Card_Order_Request',
NOT(ISNEW()),
NOT(OR(
Mandatory_Source_EPA__c ,
EPA_Store_Stock_Supply__c ,
JWOD_Programs_NB_NISH__c ,
GSA_GSA_Advantage_General_Services__c ,
FSS_Federal_Supply_Sources__c ,
Sole_Source_Purchase__c 
))
)</errorConditionFormula>
        <errorDisplayField>Mandatory_Source_EPA__c</errorDisplayField>
        <errorMessage>Select Mandatory Sources.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PCOR_Validate_SamsLink</fullName>
        <active>true</active>
        <description>PCOR Application Created to validate the Sams link and if it is checked, user need to enter sams date.</description>
        <errorConditionFormula>AND(
 OR(RecordType.DeveloperName =='Purchase_Card_Order_Request',
RecordType.DeveloperName =='PCOR_Amendment'),
SAM_Link__c,
ISPICKVAL(Request_Status__c,'Awaiting Approval'),
(ISBLANK(SAMS_Check_Date__c))
)</errorConditionFormula>
        <errorDisplayField>SAMS_Check_Date__c</errorDisplayField>
        <errorMessage>'SAMS Check Date' should be filled when 'SAM Link' is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PCOR_Validate_Shipping_Phone</fullName>
        <active>false</active>
        <description>PCOR Application. Created to validate phone.</description>
        <errorConditionFormula>AND(
RecordType.DeveloperName =='Purchase_Card_Order_Request',
NOT(ISBLANK(Shipping_Phone__c )),
NOT(CONTAINS(Shipping_Phone__c, "(XXX) XXX-XXXX")),
NOT(REGEX(Shipping_Phone__c ,"(\\D?[0-9]{3}\\D?)[\\s][0-9]{3}-[0-9]{4}"))
)</errorConditionFormula>
        <errorDisplayField>Shipping_Phone__c</errorDisplayField>
        <errorMessage>Invalid Phone Number. Enter in the format (XXX) XXX-XXXX.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PCOR_Validate_Shipping_ZipCode</fullName>
        <active>false</active>
        <description>PCOR Application. Created to validate the shipping zip code of the order request.</description>
        <errorConditionFormula>IF(
AND(
RecordType.DeveloperName ='Purchase_Card_Order_Request',
NOT(ISBLANK(Shipping_Zip_Postal_Code__c )),
NOT(REGEX(  Shipping_Zip_Postal_Code__c , "[0-9]{5}"))
),
true,false)</errorConditionFormula>
        <errorDisplayField>Shipping_Zip_Postal_Code__c</errorDisplayField>
        <errorMessage>Invalid Zip Code.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PCOR_Validate_Vendor_ZipCode</fullName>
        <active>false</active>
        <description>PCOR Application. Created to validate the zip code in the vendor details.</description>
        <errorConditionFormula>IF(
AND(
RecordType.DeveloperName ='Purchase_Card_Order_Request',
NOT(ISBLANK(Vendor_Zip__c )),
NOT(REGEX( Vendor_Zip__c , "[0-9]{5}"))
),
true,false)</errorConditionFormula>
        <errorDisplayField>Vendor_Zip__c</errorDisplayField>
        <errorMessage>Invalid Zip Code.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PCOR_Validate_Website</fullName>
        <active>true</active>
        <description>PCOR Application Created to validate the website.</description>
        <errorConditionFormula>AND(
RecordType.DeveloperName ='Purchase_Card_Order_Request',
NOT(ISBLANK(Website__c )),
NOT(BEGINS(Website__c , "https://")),
NOT(BEGINS(Website__c , "http://")),
NOT(BEGINS(Website__c , "www.")))</errorConditionFormula>
        <errorDisplayField>Website__c</errorDisplayField>
        <errorMessage>Invalid Website.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SAMS_Link_Mandatory</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
ISPICKVAL(Request_Status__c,'Awaiting Approval'),
$RecordType.Name = "Purchase Card Order Request",
SAM_Link__c
)</errorConditionFormula>
        <errorDisplayField>SAM_Link__c</errorDisplayField>
        <errorMessage>System of Award Management field is required.</errorMessage>
    </validationRules>
